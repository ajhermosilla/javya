<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude encrypted shared preferences (auth tokens) from cloud backup -->
        <exclude domain="sharedpref" path="javya_auth_prefs.xml" />
    </cloud-backup>
    <device-transfer>
        <!-- Exclude encrypted shared preferences from device transfer -->
        <exclude domain="sharedpref" path="javya_auth_prefs.xml" />
    </device-transfer>
</data-extraction-rules>
